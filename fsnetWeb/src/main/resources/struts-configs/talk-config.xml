<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<include file="struts-default.xml" />

	<package name="talk" extends="struts-default">
		<action name="TalkMembersActivate" class="fr.univartois.ili.fsnet.actions.TalkMembers"
			method="activate" />
		<action name="TalkMembersReceive" class="fr.univartois.ili.fsnet.actions.TalkMembers"
			method="receive" />
		<action name="TalkMembersSend" class="fr.univartois.ili.fsnet.actions.TalkMembers"
			method="send" />
		<action name="TalkMembersGetTalks" class="fr.univartois.ili.fsnet.actions.TalkMembers"
			method="getTalks" />
		<action name="TalkMembersGetTalk" class="fr.univartois.ili.fsnet.actions.TalkMembers"
			method="getTalk" />
		<action name="TalkMembersCloseTalk" class="fr.univartois.ili.fsnet.actions.TalkMembers"
			method="closeTalk" />
	</package>
</struts>

