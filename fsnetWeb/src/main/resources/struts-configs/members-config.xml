<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<include file="struts-default.xml" />
	<package name="members-default" extends="struts-default">
		<result-types>
			<result-type name="tiles"
				class="org.apache.struts2.views.tiles.TilesResult" />
		</result-types>

		<action name="SearchMember" class="fr.univartois.ili.fsnet.actions.ManageMembers"
			method="search">
			<result name="success" type="tiles">/Research</result>
			<result name="input">/Members</result>
		</action>

		<action name="ListMembersOfGroup" class="fr.univartois.ili.fsnet.actions.ManageMembers"
			method="search">
			<result name="success" type="tiles">SearchMembers</result>
			<result name="input">/Members</result>
		</action>

		<action name="ListMembers" class="fr.univartois.ili.fsnet.actions.ManageMembers"
			method="search">
			<result name="success" type="tiles">SearchMembers</result>
		</action>

	</package>
</struts>
