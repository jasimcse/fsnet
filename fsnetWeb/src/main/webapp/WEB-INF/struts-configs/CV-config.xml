<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts-config PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
"http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
	<form-beans>

		<form-bean name="CVForm"
			type="org.apache.struts.validator.DynaValidatorActionForm">
			<form-property name="CvBirthDay" type="java.lang.String" />
			<form-property name="CvTitle" type="java.lang.String" />
			<form-property name="CvNom" type="java.lang.String" />
			<form-property name="CvPrenom" type="java.lang.String" />
			<form-property name="CvAdresse" type="java.lang.String" />
			<form-property name="CvVille" type="java.lang.String" />
			<form-property name="CvCp" type="java.lang.String" />
			<form-property name="CvPays" type="java.lang.String" />
			<form-property name="CvTel" type="java.lang.String" />
			<form-property name="CvSituation" type="java.lang.String" />
			<form-property name="CvContact" type="java.lang.String" />
			<form-property name="CvPoste" type="java.lang.String" />
			<form-property name="NomEntreprise" type="java.lang.String" />
			<form-property name="expEndDate" type="java.lang.String" />
			<form-property name="expBeginDate" type="java.lang.String" />
			<form-property name="CvSecteur" type="java.lang.String" />
			<form-property name="CvLieu" type="java.lang.String" />
			<form-property name="CvEtude" type="java.lang.String" />
			<form-property name="CvEtudeDom" type="java.lang.String" />
			<form-property name="CvEtablissment" type="java.lang.String" />
			<form-property name="CvEtudePays" type="java.lang.String" />
			<form-property name="CvEtudeVille" type="java.lang.String" />
			<form-property name="etudEndDate" type="java.lang.String" />
			<form-property name="etudBeginDate" type="java.lang.String" />
			<form-property name="CvNomLoisir" type="java.lang.String" />
			<form-property name="CvFin" type="java.lang.String" />
			<form-property name="DateObtention" type="java.lang.String" />
			<form-property name="CvEtablissmentform" type="java.lang.String" />
			<form-property name="CvFormation" type="java.lang.String" />
			<form-property name="CvFormPays" type="java.lang.String" />
			<form-property name="CvFormVille" type="java.lang.String" />
			<form-property name="CvVilleExp" type="java.lang.String" />
			<form-property name="CvPaysExp" type="java.lang.String" />
		</form-bean>

		<form-bean name="GenerateForm"
			type="org.apache.struts.validator.DynaValidatorActionForm">
		</form-bean>

	</form-beans>

	<action-mappings>
		<action path="/CreateCv1" type="fr.univartois.ili.fsnet.actions.ManageCV"
			input="/CreateCv1.do" scope="request" name="CVForm" validate="true"
			parameter="Cree">
			<forward name="success" path="InfoProfileCv" />
		</action>
		<action path="/CreateCv2" type="fr.univartois.ili.fsnet.actions.ManageCV"
			input="/CreateCv2.do" scope="request" name="CVForm" validate="true"
			parameter="displayProfile">
			<forward name="success" path="InfoExpCv" />
			<forward name="unauthorized" path="InfoProfileCv" />
		</action>
		<action path="/CreateCv3" type="fr.univartois.ili.fsnet.actions.ManageCV"
			input="/DisplayProfile.do" scope="request" name="CVForm" validate="true"
			parameter="displayExp">
			<forward name="success" path="/DisplayProfile.do" />

		</action>

		<action path="/DisplayCV" type="fr.univartois.ili.fsnet.actions.ManageCV"
			input="/DisplayCV.do" validate="true" parameter="displayCv">
			<forward name="success" path="DisplayCv" />
		</action>

		<action path="/Generate" name="GenerateForm"
			type="fr.univartois.ili.fsnet.actions.GenerateCv" scope="request"
			validate="true" parameter="download">
		</action>

	</action-mappings>
</struts-config>
		
