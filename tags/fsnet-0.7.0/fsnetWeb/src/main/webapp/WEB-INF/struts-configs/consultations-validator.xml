<!DOCTYPE form-validation PUBLIC
"-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1.3//EN"
"http://jakarta.apache.org/commons/dtds/validator_1_1_3.dtd">
<form-validation>
	<formset>
		<form name="/CreateConsultation">
			<field property="consultationTitle" depends="required">
				<arg key="consultation.title" name="required"/>
			</field>
			<field property="consultationChoice" depends="required">
				<arg key="consultation.choice" name="required"/>
			</field>
		</form>
	</formset>
</form-validation>