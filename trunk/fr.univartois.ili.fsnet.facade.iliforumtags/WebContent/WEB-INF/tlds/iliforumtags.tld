<?xml version="1.0" encoding="UTF-8" ?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
	version="2.0">
	<description>IliForum Tags</description>
	<display-name>IliForum Tags</display-name>
	<tlib-version>1.0</tlib-version>
	<short-name>iliforum</short-name>
	<uri>http://iliforum.ili.fsnet.com/</uri>
	<tag>
		<display-name>Display Hub</display-name>
		<name>hub</name>
		<tag-class>fr.univartois.ili.fsnet.facade.iliforumtags.HubTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>var</name>
			<required>yes</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>dateBegin</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>dateEnd</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>decideur</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<display-name>Display Topic</display-name>
		<name>topic</name>
		<tag-class>fr.univartois.ili.fsnet.facade.iliforumtags.TopicTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>var</name>
			<required>yes</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>dateBegin</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>dateEnd</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>user</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>hub</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

<tag>
		<display-name>Display Message</display-name>
		<name>message</name>
		<tag-class>fr.univartois.ili.fsnet.facade.iliforumtags.MessageTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>var</name>
			<required>yes</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>dateBegin</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>dateEnd</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>user</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>topic</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

</taglib>